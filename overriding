package classes;

	class animal{
		void sound(){
			System.out.println("Animal makes sound");
		}
	}
	class DOG extends animal{
		void sound() {
			System.out.println("Dog barks");
		}
		
	}
	public class overriding {
	public static void main(String[] args) {
		animal ob1 = new DOG();
		ob1.sound();
	}

}
